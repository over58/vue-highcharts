{
  "name": "highcharts-vue-xyc",
  "version": "1.0.7",
  "description": "Highcharts wrapper Vue component",
  "main": "dist/highcharts-vue-xyc.min.js",
  "typings": "types/highcharts-vue-xyc.d.ts",
  "scripts": {
    "test": "jest",
    "build": "webpack",
    "build:app": "cd demo && npm install",
    "app": "cd demo && npm run dev",
    "lint": "eslint . --ext .js,.vue",
    "lint:fix": "eslint . --ext .js,.vue --fix",
    "release": "standard-version"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  },
  "homepage": "https://github.com/xuyongchaos/vue-highcharts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xuyongchaos/vue-highcharts.git"
  },
  "bugs": {
    "url": "https://github.com/xuyongchaos/vue-highcharts/issues"
  },
  "keywords": [
    "highcharts",
    "wrapper",
    "vue",
    "component"
  ],
  "author": "Tim上校的的小屋 <2658553345@qq.com>",
  "license": "SEE LICENSE IN LICENSE",
  "files": [
    "dist",
    "src",
    "types",
    "utils"
  ],
  "peerDependencies": {
    "highcharts": ">=5.0.0",
    "vue": ">=1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.39",
    "@babel/preset-env": "^7.0.0-beta.39",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.0-beta.0",
    "eslint": "^4.17.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.2.2",
    "highcharts": "^7.1.1",
    "husky": "^3.0.8",
    "jest": "^24.8.0",
    "jest-serializer-vue": "^2.0.2",
    "standard": "^10.0.3",
    "standard-version": "^4.4.0",
    "vue": "^2.6.10",
    "vue-jest": "^3.0.4",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "lodash": "^4.17.15"
  }
}
